    </view>
  </view>

  <view class='flex-center-start-column'>
    <text class='tips fz15 fw700'>
      <text class='co-f0'></text>相邻人同意设置的书面协议
    </text>

    <view bindtap='uploadFn1' data-tap='7' class="flex-center-column w100 content body fz14 co-99">
      <text wx-if="{{!img7}}">拍照上传</text>
      <image wx-if="{{!img7}}" style='height : 24px; width : 24px;' src='../image/icon_add.png'></image>
      <image wx-if="{{img7}}" mode='aspectFit' src='{{img7}}'></image>
    </view>
  </view>

  <view class='flex-center-start-column'>
    <text class='tips fz15 fw700'>
      <text class='co-f0'></text>原准予行政许可决定书和原效果图复印件
    </text>

    <view bindtap='uploadFn1' data-tap='8' class="flex-center-column w100 content body fz14 co-99">
      <text wx-if="{{!img8}}">拍照上传</text>
      <image wx-if="{{!img8}}" style='height : 24px; width : 24px;' src='../image/icon_add.png'></image>
      <image wx-if="{{img8}}" mode='aspectFit' src='{{img8}}'></image>
    </view>
  </view>

  <!-- <button bindtap='submit' class='btn co-ff fz14'>提交申请</button> -->

     <view wx:if="{{!disabled}}" class='flex-center-warp bottom fz14 white'>
      <view class='back co-ff' bindtap='back'>返回</view>
      <view class='next co-ff' bindtap='submit'>提交</view>
    </view>
</view>